                    xmlns:ui="using:FluentAvalonia.UI.Controls">

  <ui:SymbolIconSource x:Key="SaveIcon" Symbol="Save" />
  <ui:FontIconSource x:Key="FontIcon" Glyph="F" />
  <ui:PathIconSource x:Key="PathIcon" Data="M0,0 L 50,0 L 25,50Z" />
